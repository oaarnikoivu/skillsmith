      "id": "search_slots_slots_search_get",
      "summary": "Search Slots",
      "method": "GET",
      "path": "/slots/search",
      "tags": [],
      "parameters": [
        {
          "name": "start",
          "location": "query",
          "required": true,
          "schemaSummary": "string(date)"
        },
        {
          "name": "end",
          "location": "query",
          "required": true,
          "schemaSummary": "string(date)"
        },
        {
          "name": "club_id",
          "location": "query",
          "required": false,
          "schemaSummary": "nullable array<string>"
        },
        {
          "name": "court_type",
          "location": "query",
          "required": false,
          "schemaSummary": "nullable enum(indoor, outdoor, unknown)",
          "enumValues": ["indoor", "outdoor", "unknown"]
        },
        {
          "name": "group_mode",
          "location": "query",
          "required": false,
          "schemaSummary": "enum(none, club, time, time_club, time_of_day, time_of_day_club)",
          "defaultValue": "none",
          "enumValues": ["none", "club", "time", "time_club", "time_of_day", "time_of_day_club"]
        },
        {
          "name": "limit",
          "location": "query",
          "required": false,
          "schemaSummary": "integer",
          "defaultValue": 200
        },
        {
          "name": "offset",
          "location": "query",
          "required": false,
          "schemaSummary": "integer",
          "defaultValue": 0
        }
      ],
      "responses": [
        {
          "statusCode": "200",
          "description": "Successful Response",
          "schemaSummary": "SlotsSearchOut"
        },
        {
          "statusCode": "422",
          "description": "Validation Error",
          "schemaSummary": "HTTPValidationError"
        }
      ]
    },
